def bubble_sort(list):
    print("До:", list)
    n = len(list)
    for i in range(n):
        for j in range(0, n - i - 1):
            if list[j] > list[j + 1]:
                list[j], list[j + 1] = list[j + 1], list[j]
    print("После:", list)


l = [64, 90, 34, 25, 12, 22, 11 ]
bubble_sort(l)
